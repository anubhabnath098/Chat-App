{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/messages(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/socket.io(.*)",
        "dest": "/index.js"
      }
    ]

  }
  